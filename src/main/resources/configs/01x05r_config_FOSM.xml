<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v2.dtd">
<config>
	<module name="taxi">
		<param name="taxisFile" value="src/main/resources/amod/amod_vehicles_OSM.xml" />

		<param name="pickupDuration" value="120"/>
		<param name="dropoffDuration" value="60"/>
		<param name="vehicleDiversion" value="true" />
		<param name="onlineVehicleTracker" value="true" />
		<param name="destinationKnown" value="true" />
		<param name="timeProfiles" value="true" />
		<param name="detailedStats" value="true" />

		<parameterset type="optimizer">
			<param name="type" value="AMoD" />
			<param name="id" value="AMoD" />
			<param name="reoptimizationTimeStep" value="300"/>
			<param name="nearestRequestsLimit" value="99999" />
			<param name="nearestVehiclesLimit" value="99999" />
			<param name="cellSize" value="1000" />
			<param name="optimizerDelay" value="15" />
			<param name="rebalanceWeight" value="1.00"/>
			<param name="optimizerDataFile" value="src/main/resources/optimizerdata/March1SourcesSinksNodes.mat" />
			<param name="timeHorizon" value="1200" />
			<param name="tripThreshold" value="1" />
			<param name="legacyRebalance" value="false" />
			<param name="amodDispatch" value="true" />
			<param name="neighbourhoodSize" value="150" />
			<param name="reschedulePeriod" value="60" />
			<param name="stationMap" value="src/main/resources/station_node_map_NYOSM.mat" />
			<!--<param name="endTime" value="79200" /> -->
		</parameterset>
	</module>

	<module name="network">
		<param name="inputNetworkFile" value="src/main/resources/NYOSMnetwork_25cap.xml" />
	</module>

	<module name="plans">
		<param name="inputPlansFile" value="src/main/resources/plans/march05plansOSM.xml.gz" />
	</module>

	<module name="qsim">
		<param name="simStarttimeInterpretation" value="onlyUseStarttime" />
		<param name="insertingWaitingVehiclesBeforeDrivingVehicles" value="true" />
		<param name="snapshotStyle" value="queue" />
		<param name="startTime" value="00:00:00" />

		<param name="timeStepSize" value="00:00:01" />
		<!--<param name="endTime" value="21:00:0"/>-->
	</module>

	<module name="otfvis">
		<param name="coloringScheme" value="taxicab" />
		<param name="drawNonMovingItems" value="false" />
	</module>

	<module name="controler">
		<param name="outputDirectory" value="test/output/01x05_pax-reb_congestion_OSM_2h_20170203_1334_randsourcesinks" />
		<param name="overwriteFiles" value="deleteDirectoryIfExists" />
		<param name="firstIteration" value="0" />
		<param name="lastIteration" value="0" />
	</module>

	<module name="planCalcScore">
		<param name="activityType_0" value="dummy" />
		<param name="activityTypicalDuration_0" value="24:00:00" />
	</module>

	<module name="strategy">
		<!-- Either we have only DynAgents, or we want "standard" agents to serve as a background,
		i.e. not to change their behavior -->
		<param name="maxAgentPlanMemorySize" value="1" />
		<param name="ModuleProbability_1" value="1.0" />
		<param name="Module_1" value="BestScore" />
	</module>
</config>
